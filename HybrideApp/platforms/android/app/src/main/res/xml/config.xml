<?xml version='1.0' encoding='utf-8'?>
<widget id="com.company.Name" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="MainServices">
        <param name="android-package" value="com.company.Name.MainServices" />
    </feature>
    <feature name="ProviderPlugins">
        <param name="android-package" value="com.company.Name.ProviderPlugins" />
    </feature>
    <name>NativeToCordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <config-file parent="/*" target="config.xml">
    </config-file>
    <preference name="loglevel" value="DEBUG" />
</widget>
